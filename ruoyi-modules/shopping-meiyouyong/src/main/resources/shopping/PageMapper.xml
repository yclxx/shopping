<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dromara.shopping.mapper.PageMapper">

    <resultMap type="org.dromara.shopping.domain.Page" id="PageResult">
        <result property="id" column="id"/>
        <result property="pagePath" column="page_path"/>
        <result property="pageName" column="page_name"/>
        <result property="navBackgroundColor" column="nav_background_color"/>
        <result property="appletStyle" column="applet_style"/>
        <result property="appletTitleBarVisible" column="applet_title_bar_visible"/>
        <result property="status" column="status"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="platformKey" column="platform_key"/>
    </resultMap>


</mapper>
